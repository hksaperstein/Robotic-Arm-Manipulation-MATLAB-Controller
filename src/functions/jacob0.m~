function [ jaco] = jacob0(q)

A = 135;
B = 175;
C = 169.28;



J = ...
[ -cos(q(1))*(C*cos(q(2) + q3(t)) + B*cos(q(2))), sin(q(1))*(C*sin(q(2) + q3(t)) + B*sin(q2(t))), C*sin(q(1))*sin(q2(t) + q3(t));
  -sin(q(1))*(C*cos(q(2) + q3(t)) + B*cos(q(2))),-cos(q(1))*(C*sin(q(2) + q3(t)) + B*sin(q2(t))), -C*cos(q(1))*sin(q2(t) + q3(t));
  0, C*cos(q(2) + q3(t)) + B*cos(q2(t)), C*cos(q(2) + q3(t));
  0, sin(pi/2 + q(1)), -cos(pi/2 + q(1))*sin(q(2));
  0, -cos(pi/2 + q(1)), -sin(q(2))*sin(pi/2 + q1(t));
  1, 0, cos(q2(t))];

end

