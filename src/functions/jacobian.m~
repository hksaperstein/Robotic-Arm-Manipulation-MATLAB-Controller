function [ jaco] = jacobian(q)

L1 = 135;
L2 = 175;
L3 = 169.28;

alpha1 = 90;
alpha2 = 0;
alpha3 = 0;

%conversion = pi / 2048; %degrees / ticks
%q = q * 360/4096;
                    %d, a, theta, alpha
dh1 = dhTransform(L1, 0, -q(1), alpha1);
dh2 = dhTransform(0, L2, q(2), alpha2);
dh3 = dhTransform(0, L3, q(3)-90, alpha3);

dh01 = dh1;
dh02 = dh1*dh2;
dh03 = dh02*dh3;

diffq1 = diff(dh03(1:3,4), q(1));
diffq2 = diff(dh03(1:3,4), q(2));
diffq3 = diff(dh03(1:3,4), q(3));

jaco = [diffq1 diffq2 diffq3; [0; 0; 1] dh01(1:3, 3) dh02(1:3, 3)];


end

